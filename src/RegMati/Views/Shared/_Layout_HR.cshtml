        <script>
            $(window).on('load', function () {

                //var txt = $('#ck_isNull').val();

                //if (txt == "N") {

                //    window.location.assign('../../Login/Index');

                //}


                $.post('../../Home/gnt')
                .done(function (data) {

                    var res = data.split('^');

                    var n_pro = res[0];
                    var n_chg_ps = res[1];
                    var n_chg_emp = res[2];
                    var n_rsg_emp = res[3];
                    // alert(data);

                    if (n_pro == "0") {

                        $('.n_pro').html('');

                    } else {

                        $('.n_pro').html('<span class="label label-success pull-right">' + n_pro + '</span>');
                    }

                    if (n_chg_ps == "0") {

                        $('.n_chg_ps').html('');

                    } else {

                        $('.n_chg_ps').html('<span class="label label-warning pull-right">' + n_chg_ps + '</span>');
                    }

                    if (n_chg_emp == "0") {

                        $('.n_chg_emp').html('');
                    } else {

                        $('.n_chg_emp').html('<span class="label label-primary pull-right">' + n_chg_emp + '</span>');
                    }

                    if (n_rsg_emp == "0") {

                        $('.n_rsg_emp').html('');

                    } else {

                        $('.n_rsg_emp').html('<span class="label label-danger pull-right">' + n_rsg_emp + '</span>');

                    }

                })


            })

            $('.data_table').DataTable();
            //Date picker
            $('.datepicker').datepicker({
                autoclose: true,
                language: 'th-th',
                format: 'dd/mm/yyyy',
                isBuddhist: true
            });
            $('.select2').select2()
        </script>
        <script src="~/lib/plugins/iCheck/icheck.min.js"></script>
        <!-- Page Script -->
        <script>
            $(function () {
                //Enable iCheck plugin for checkboxes
                //iCheck for checkbox and radio inputs
                $('.mailbox-messages input[type="checkbox"]').iCheck({
                    checkboxClass: 'icheckbox_flat-blue',
                    radioClass: 'iradio_flat-blue'
                });

                //Enable check and uncheck all functionality
                $(".checkbox-toggle").click(function () {
                    var clicks = $(this).data('clicks');
                    if (clicks) {
                        //Uncheck all checkboxes
                        $(".mailbox-messages input[type='checkbox']").iCheck("uncheck");
                        $(".fa", this).removeClass("fa-check-square-o").addClass('fa-square-o');
                    } else {
                        //Check all checkboxes
                        $(".mailbox-messages input[type='checkbox']").iCheck("check");
                        $(".fa", this).removeClass("fa-square-o").addClass('fa-check-square-o');
                    }
                    $(this).data("clicks", !clicks);
                });

                //Handle starring for glyphicon and font awesome
                $(".mailbox-star").click(function (e) {
                    e.preventDefault();
                    //detect type
                    var $this = $(this).find("a > i");
                    var glyph = $this.hasClass("glyphicon");
                    var fa = $this.hasClass("fa");

                    //Switch states
                    if (glyph) {
                        $this.toggleClass("glyphicon-star");
                        $this.toggleClass("glyphicon-star-empty");
                    }

                    if (fa) {
                        $this.toggleClass("fa-star");
                        $this.toggleClass("fa-star-o");
                    }
                });
            });
        </script>
        <script>
            $(function () {
                //Enable iCheck plugin for checkboxes
                //iCheck for checkbox and radio inputs
                $('.mailbox-messages-check input[type="checkbox"]').iCheck({
                    checkboxClass: 'icheckbox_flat-red',
                    radioClass: 'iradio_flat-red'
                });

                //Enable check and uncheck all functionality
                $(".checkbox-toggle").click(function () {
                    var clicks = $(this).data('clicks');
                    if (clicks) {
                        //Uncheck all checkboxes
                        $(".mailbox-messages-check input[type='checkbox']").iCheck("uncheck");
                        $(".fa", this).removeClass("fa-check-square-o").addClass('fa-square-o');
                    } else {
                        //Check all checkboxes
                        $(".mailbox-messages-check input[type='checkbox']").iCheck("check");
                        $(".fa", this).removeClass("fa-square-o").addClass('fa-check-square-o');
                    }
                    $(this).data("clicks", !clicks);
                });

                //Handle starring for glyphicon and font awesome

            });
            //iCheck for checkbox and radio inputs
            $('input[type="checkbox"].minimal, input[type="radio"].minimal').iCheck({
                checkboxClass: 'icheckbox_minimal-blue',
                radioClass: 'iradio_minimal-blue'
            })
            //Red color scheme for iCheck
            $('input[type="checkbox"].minimal-red, input[type="radio"].minimal-red').iCheck({
                checkboxClass: 'icheckbox_minimal-red',
                radioClass: 'iradio_minimal-red'
            })
            //Flat red color scheme for iCheck
            $('input[type="checkbox"].flat-red, input[type="radio"].flat-red').iCheck({
                checkboxClass: 'icheckbox_flat-green aa',
                radioClass: 'iradio_flat-green'
            })

        </script>
